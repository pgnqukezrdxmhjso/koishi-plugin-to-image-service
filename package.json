{
  "name": "koishi-plugin-to-image-service",
  "description": "Generate images using a non-puppeteer solution",
  "version": "0.1.5",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "cd ../../ && yarn build to-image-service",
    "pub": "cd ../../ && yarn pub to-image-service",
    "fastPub": "cd ../../ && yarn bump -3 to-image-service && yarn build to-image-service && yarn pub to-image-service"
  },
  "files": [
    "lib",
    "dist",
    "assets/noto-sans-v27-latin-regular.ttf"
  ],
  "contributors": [
    "pgnqukezrdxmhjso"
  ],
  "homepage": "https://github.com/pgnqukezrdxmhjso/koishi-plugin-to-image-service",
  "bugs": {
    "url": "https://github.com/pgnqukezrdxmhjso/koishi-plugin-to-image-service/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgnqukezrdxmhjso/koishi-plugin-to-image-service.git"
  },
  "license": "MIT",
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "to image",
    "renderer",
    "satori",
    "html to image",
    "jsx to image"
  ],
  "peerDependencies": {
    "koishi": "^4.18.10"
  },
  "dependencies": {
    "@resvg/resvg-wasm": "^2.6.2",
    "canvg": "^4.0.3",
    "html-react-parser": "^5.2.11",
    "jsdom": "26.1.0",
    "react": "^19.2.3",
    "satori": "^0.18.3",
    "sucrase": "^3.35.1",
    "wasm-vips": "^0.0.16"
  },
  "devDependencies": {
    "@napi-rs/canvas": "^0.1.88",
    "@types/jsdom": "^27",
    "@types/react": "^19",
    "koishi-plugin-w-node": "^0.2.2"
  },
  "koishi": {
    "service": {
      "required": [
        "node"
      ],
      "implements": [
        "toImageService"
      ]
    }
  }
